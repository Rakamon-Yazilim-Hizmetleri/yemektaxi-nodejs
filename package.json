{
  "name": "yemektaxi-backend",
  "author": "bch",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "generate": "prisma generate",
    "build": "tsc && npm run sentry:sourcemaps",
    "build:dev": "tsc",
    "start": "docker compose -f docker-compose.prod.yml up -d --build",
    "migarete": "npx prisma migrate dev",
    "start:prod": "node ./dist/index.js",
    "seed": "ts-node ./src/seed.ts",
    "start:migrate": "prisma migrate deploy && npm run start:prod",
    "dev": "docker compose up -d && tsnd ./src/index.ts",
    "dev:local": "tsnd ./src/index.ts",
    "swagger:validate": "npx swagger-jsdoc --definition src/utils/swagger.ts --apis 'src/routes/*.ts' --output swagger.json",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org univerlist-sm --project node ./dist && sentry-cli sourcemaps upload --org univerlist-sm --project node ./dist"
  },
  "prisma": {
    "seed": "ts-node ./src/seed.ts"
  },
  "eslintIgnore": [
    "/node_modules"
  ],
  "nodemonConfig": {
    "ignore": [
      "src/generated/**/*"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/lib-storage": "^3.379.1",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@craftgate/craftgate": "^1.0.40",
    "@prisma/client": "^5.9.1",
    "@sentry/cli": "^2.38.2",
    "@sentry/node": "^7.120.0",
    "@sentry/tracing": "^7.44.2",
    "@sentry/types": "^7.40.0",
    "adm-zip": "^0.5.10",
    "argon2": "^0.30.3",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.1",
    "bull-arena": "^3.29.3",
    "bullmq": "^3.10.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.35.0",
    "cors": "^2.8.5",
    "cuid": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "firebase-admin": "^11.11.1",
    "form-data": "^4.0.0",
    "i": "^0.3.7",
    "import": "^0.0.6",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.30.1",
    "nanoid": "^3.3.6",
    "node-fetch": "^2.6.7",
    "npm": "^10.8.0",
    "sib-api-v3-sdk": "^8.5.0",
    "stream-to-string": "^1.2.1",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.5.1",
    "ulid": "^2.3.0",
    "winston": "^3.8.2",
    "xml2js": "^0.6.2",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bull-arena": "^3.0.7",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.37.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.2.1",
    "@types/object-hash": "^3.0.2",
    "@types/streamifier": "^0.1.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "prisma": "^5.9.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14"
  },
  "main": ".eslintrc.js",
  "license": "ISC",
  "description": ""
}
